    $(document).on('click', '#btn_Editar', function () {
        $('#editarModal').modal('toggle');

        var idTurno = $(this).parents().closest('tr').attr('id')
        $.ajax({
            type: 'POST',
            url: '@Url.Action("GetTurno")',
            data: {
                id: idTurno
            },
            cache: false,
            success: function (result) {
                $('#editarModal').find('input#input-Id').val(result.Id);
                $('#editarModal').find('input#input-DNI-Edit').val(result.DNI);
                $('#editarModal').find('input#input-Apellido-Edit').val(result.Apellido);
                $('#editarModal').find('input#input-Nombre-Edit').val(result.Nombre);
                $('#editarModal').find('#selectTipoEdit').val(result.TipoTramite);
                $('#editarModal').find('input#input-FechaTurno-Edit').val(result.FechaTurno);
            },
            error: function (response) {
                    Lobibox.notify('error', {
                        title: 'Error',
                        msg: 'Ah ocurrido un error, intente nuevamente.'
                });
                    $('#editarModal').modal('hide');
            }
        });
    });

    $(document).on('click', '#btn_Ver', function () {
        $('#editarModal').modal('toggle');

