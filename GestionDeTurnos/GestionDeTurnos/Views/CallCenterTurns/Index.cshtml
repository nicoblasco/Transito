
@model IEnumerable<GestionDeTurnos.ViewModel.CallCenterIndexViewModel>


@if (TempData["message"] != null)
{
    if (TempData["message"].ToString().Contains("Lo sentimos"))
    {
        <div>
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="alert alert-danger alert-dismissable">
                        <button id="btndismissable" type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>@TempData["message"]
                    </div>
                </div>
            </div>
        </div>
    }
    else
    {
        <div>
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="alert alert-success alert-dismissable">
                        <button id="btndismissable" type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>@TempData["message"]
                    </div>
                </div>
            </div>
        </div>
    }
}


<!-- Progress Loader -->
<div id="div-loader" style="margin: 0px; padding: 0px; position: fixed; right: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(102, 102, 102); z-index: 30001; opacity: 0.8; display: none">
    <p style="position: absolute; color: White; top: 35%; left: 45%;">
        <img src="~/Images/loading.gif" alt="Loading..." />
    </p>
</div>


@Html.Partial("_Filtros");
@Html.Partial("_Table");


<!-- Modal -->

<div class="modal fade" id="nuevoModal" role="dialog">
    <div class="modal-dialog">
        <form id="form-create" method="post">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Nuevo Turno</h4>
                </div>
                <div class="modal-body">
                    <form role="form">
                        <div class="form-group">
                            <label>DNI</label>
                            <input id="input-DNI" name="inputDNI" class="form-control" autocomplete="off">
                        </div>
                        <div class="form-group">
                            <label>Apellido</label>
                            <input id="input-Apellido" name="inputApellido" class="form-control" autocomplete="off">
                        </div>
                        <div class="form-group">
                            <label>Nombre</label>
                            <input id="input-Nombre" name="inputNombre" class="form-control" autocomplete="off">
                        </div>
                        <div class="form-group">
                            <label>Tipo de Tramite</label>
                            @Html.DropDownList("selectTipoCreate", new SelectList(ViewBag.listaLicencias, "Referencia", "Descripcion"), new { @class = "form-control" })
                        </div>
                        <div class="form-group">
                            <label>Fecha y Hora del Turno</label>
                            <input class="form-control input-sm input-datelong" id="input-FechaTurno" type="text" name="inputFechaTurno">
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    <button type="submit" class="btn btn-primary">Guardar</button>
                </div>
            </div>
        </form>
    </div>
</div>

<div class="modal fade" id="editarModal" role="dialog">
    <div class="modal-dialog">
        <form id="form-edit" method="post">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Editar Turno</h4>
                </div>
                <div class="modal-body">
                    <form role="form">
                        <div class="form-group" style="display:none">
                            <label>ID</label>
                            <input id="input-Id" name="inputId" class="form-control" autocomplete="off">
                        </div>
                        <div class="form-group">
                            <label>DNI</label>
                            <input id="input-DNI-Edit" name="inputDNI" class="form-control" autocomplete="off">
                        </div>
                        <div class="form-group">
                            <label>Apellido</label>
                            <input id="input-Apellido-Edit" name="inputApellido" class="form-control" autocomplete="off">
                        </div>
                        <div class="form-group">
                            <label>Nombre</label>
                            <input id="input-Nombre-Edit" name="inputNombre" class="form-control" autocomplete="off">
                        </div>
                        <div class="form-group">
                            <label>Tipo de Tramite</label>
                            @Html.DropDownList("selectTipoEdit", new SelectList(ViewBag.listaLicencias, "Referencia", "Descripcion"), new { @class = "form-control" })
                        </div>
                        <div class="form-group">
                            <label>Fecha y Hora del Turno</label>
                            <input class="form-control input-sm input-datelong" id="input-FechaTurno-Edit" type="text" name="inputFechaTurno">
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    <button type="submit" class="btn btn-primary">Guardar</button>
                </div>
            </div>
        </form>
    </div>
</div>

